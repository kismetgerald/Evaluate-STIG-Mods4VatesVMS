    if (-not $emptyPwAccts) {
        $FindingDetails += "  PASS: No accounts with empty passwords" + $nl
    }

    $FindingDetails += $nl

    # Status determination
    if ($autologinFound -or $dmAutologin -or $emptyPwIssue -or $emptyPwAccts) {
