    $FindingDetails += ("-" * 40) + $nl

    $emptyPwAccts = $false
    $shadowContent = Get-Content /etc/shadow -ErrorAction SilentlyContinue
    foreach ($line in $shadowContent) {
        if ($line -match "^([^:]+):([^:]*):") {
