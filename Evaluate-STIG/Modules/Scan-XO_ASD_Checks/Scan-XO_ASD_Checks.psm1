                $cleartextInputs = bash -c "find $webRoot -type f \`( -name '*.html' -o -name '*.js' -o -name '*.jsx' \`) -exec grep -iE 'password|passphrase|pin' {} \`; </dev/null 2>/dev/null | grep -i \`"type='text'\`" | head -5"
                
