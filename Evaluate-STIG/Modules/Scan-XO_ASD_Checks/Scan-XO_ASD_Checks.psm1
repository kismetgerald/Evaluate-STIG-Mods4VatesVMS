            "buffer.copy.*\+"
        )
        
        $unsafeFound = 0
        foreach ($pattern in $unsafePatterns) {
            $cmd = "grep -r -E '$pattern' $xoPath --include='*.js' </dev/null 2>/dev/null | head -5"
